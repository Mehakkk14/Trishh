# Production Environment Variables for TRISHH Desire Shop
# Copy this file to .env.production for production deployment

# App Configuration
VITE_APP_NAME=TRISHH
VITE_APP_URL=https://your-domain.com

# Firebase Configuration (Production)
VITE_FIREBASE_API_KEY=your_production_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# EmailJS Configuration (Production)
VITE_EMAILJS_SERVICE_ID=your_production_service_id
VITE_EMAILJS_TEMPLATE_ID=your_production_template_id
VITE_EMAILJS_PUBLIC_KEY=your_production_public_key

# Razorpay Configuration (Production)
VITE_RAZORPAY_KEY_ID=rzp_live_your_actual_key_id
VITE_RAZORPAY_KEY_SECRET=your_actual_secret_key

# SMTP Configuration (Production Backup)
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=noreply@yourdomain.com
VITE_SMTP_PASSWORD=your_production_smtp_password

# Analytics & Monitoring
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_HOTJAR_ID=your_hotjar_id

# Performance & SEO
VITE_CDN_URL=https://cdn.yourdomain.com
VITE_API_BASE_URL=https://api.yourdomain.com